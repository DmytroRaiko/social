{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "prettier",
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "plugins": ["prettier"],
    "rules": {
        "prettier/prettier": "off",
        "comma-dangle": "off",
        "operator-linebreak": "off",
        "implicit-arrow-linebreak": "off"
    }
}
